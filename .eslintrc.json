{
  "extends": "airbnb-base",
  "root": true,
  "rules": {
    "no-underscore-dangle": [1, { "allow": ["_id"] }],
    "no-console": ["error", { "allow": ["log"] }],
    "consistent-return": "off",
    "func-names": 0,
    "max-len": [2, 100],
    "quotes": ["error", "single", { "avoidEscape": true }]
  }
}
